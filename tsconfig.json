{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/firebase/config.js",
    "src/app/components/body/ContactBlocks.jsxsx",
    "src/app/components/body/FirstBlock.jsx",
    "src/app/page.tsx",
    "src/app/components/body/ForthBlock.jsx",
    "src/app/components/body/RectangleBlock.jsx",
    "src/app/components/body/SwiperComponent.jsx",
    "src/app/components/body/ThirdBlock copy.jsx",
    "src/app/components/body/ThirdBlock.jsx",
    "src/app/components/Footer.jsx",
    "src/app/components/AnimatedGlobe.jsx",
    "src/app/components/header/Header.jsx",
    "src/app/components/ContactForm.jsx",
    "src/app/components/body/SquareBlock.jsx",
    "src/app/components/body/SecondBlock.jsx",
    "src/app/agent/page.jsx",
    "src/app/about/pageS.jsx"
  ],
  "exclude": ["node_modules"]
}
